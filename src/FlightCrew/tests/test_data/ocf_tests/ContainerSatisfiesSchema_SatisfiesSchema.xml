<?xml version="1.0"?>
<!--
    From the spec:
    "Conforming container.xml files MUST be valid according
    to the OCF schema with the <container> element as the 
    root element *after* removing all elements (and child nodes
    of these elements) and attributes from other namespaces." 
-->
<container 
    version="1.0" 
    xmlns="urn:oasis:names:tc:opendocument:xmlns:container"
    xmlns:foo="foobarnamespace"
    foo:moo="test" >
    <rootfiles foo:moo="test" foo:mouuo="test" >
        <rootfile full-path="OEBPS/As You Like It.opf"
            media-type="application/oebps-package+xml"
            foo:moo="test" />
        <rootfile full-path="OEBPS/As You Like It.pdf"
            media-type="application/pdf">
            <foo:bar>
                euieu ieuie      
            </foo:bar>
        </rootfile> 
        <foo:foobar>
            <foo:bar>
                eiepie     
            </foo:bar>
        </foo:foobar>
        <foo:bar>
            euieu ieuie      
        </foo:bar>
    </rootfiles>
    <foo:foobar>
        <foo:bar>
            euieu ieuie      
        </foo:bar>
    </foo:foobar>
    <foo:bar>
        euieu ieuie      
    </foo:bar>
</container>
